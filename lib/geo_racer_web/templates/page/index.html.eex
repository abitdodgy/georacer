<section class="u-contain u-padded">
  <div class="home__container">
    <div>
      <h1><img src="<%= Routes.static_path(@conn, "/images/geo-racer-full-logo.svg") %>" class="home__logo" alt="Geo Racer Logo"/></h1>
      <img src="<%= Routes.static_path(@conn, "/images/powered-by-gaslight.svg") %>" class="gaslight-logo" alt="powered by Gaslight"/>
    </div>
    <div class="u-center">
      <a href="<%= Routes.course_path(@conn, :new) %>" class="btn btn--primary">Create Race</a>
      <a href="<%= Routes.course_path(@conn, :index) %>" class="btn u-push-bottom">Join Race</a>
      <div class="u-center">
        <a href="#">Race History</a>
      </div>
    </div>
  </div>
</section>
